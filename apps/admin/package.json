{
  "name": "@ifla/admin",
  "version": "0.1.0",
  "description": "Authentication and API services for IFLA Standards platform",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev --port 3007",
    "start": "next start --port 3007",
    "test": "vitest run",
    "test:unit": "vitest run --exclude='**/*integration*'",
    "test:integration": "vitest run --include='**/*integration*' --exclude='**/server-dependent/**'",
    "test:server-dependent": "vitest run --config=vitest.config.server-dependent.ts",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "next lint"
  },
  "devDependencies": {
    "@types/node-fetch": "^2.6.12",
    "node-fetch": "^3.3.2"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.2.0",
    "@mui/lab": "7.0.0-beta.14",
    "@mui/material": "^7.2.0",
    "@mui/x-data-grid": "^8.7.0",
    "@mui/x-tree-view": "^8.7.0",
    "date-fns": "^4.1.0"
  }
}
