    "lint": "eslint .",
    "lint:eslint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:quiet": "eslint . --quiet",
    "lint:quiet:fix": "eslint . --quiet --fix",
    "lint:file": "eslint",
    "lint:mdx": "remark . --ext .mdx,.md",
