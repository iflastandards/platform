{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "namedInputs": {
    "default": [
      "{projectRoot}/**/*",
      "!{projectRoot}/.docusaurus/**/*",
      "!{projectRoot}/build/**/*",
      "!{projectRoot}/dist/**/*",
      "!{projectRoot}/node_modules/**/*",
      "sharedGlobals"
    ],
    "production": [
      "default",
      "!{projectRoot}/**/?(*.)+(spec|test).[jt]s?(x)?(.snap)",
      "!{projectRoot}/tsconfig.spec.json",
      "!{projectRoot}/jest.config.[jt]s",
      "!{projectRoot}/.eslintrc.json",
      "!{projectRoot}/**/*.stories.@(js|jsx|ts|tsx|mdx)",
      "!{projectRoot}/src/test-setup.[jt]s"
    ],
    "docusaurus": [
      "{projectRoot}/**/*.{md,mdx}",
      "{projectRoot}/docusaurus.config.js",
      "{projectRoot}/docusaurus.config.ts",
      "{projectRoot}/sidebars.js",
      "{projectRoot}/sidebars.ts",
      "{projectRoot}/src/**/*",
      "{projectRoot}/static/**/*",
      "{workspaceRoot}/packages/theme/**/*"
    ],
    "docusaurus-no-theme": [
      "{projectRoot}/**/*.{md,mdx}",
      "{projectRoot}/docusaurus.config.js",
      "{projectRoot}/docusaurus.config.ts",
      "{projectRoot}/sidebars.js",
      "{projectRoot}/sidebars.ts",
      "{projectRoot}/src/**/*",
      "{projectRoot}/static/**/*"
    ],
    "sharedGlobals": ["{workspaceRoot}/.github/workflows/ci.yml"]
  },
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "{projectRoot}/build",
        "{projectRoot}/dist",
        "{projectRoot}/.docusaurus"
      ],
      "cache": true,
      "inputs": ["production", "^production", "docusaurus"]
    },
    "test": {
      "dependsOn": ["^build"],
      "cache": true,
      "outputs": ["{projectRoot}/coverage", "{projectRoot}/test-results"],
      "inputs": [
        "default",
        "{projectRoot}/test/**/*",
        "{projectRoot}/**/*.test.{js,ts,jsx,tsx}",
        "{projectRoot}/**/*.spec.{js,ts,jsx,tsx}",
        "{workspaceRoot}/vite.config.ts",
        "{workspaceRoot}/vitest.config.*"
      ]
    },
    "lint": {
      "cache": true,
      "inputs": [
        "default",
        "{workspaceRoot}/.eslintrc.json",
        "{workspaceRoot}/eslint.config.js",
        "{workspaceRoot}/eslint.config.mjs"
      ]
    },
    "typecheck": {
      "dependsOn": ["^build"],
      "cache": true,
      "inputs": [
        "default",
        "{projectRoot}/tsconfig.json",
        "{workspaceRoot}/tsconfig.json"
      ]
    },
    "serve": {
      "cache": false
    },
    "start": {
      "cache": false
    },
    "@nx/js:tsc": {
      "cache": true,
      "dependsOn": ["^build"],
      "inputs": ["production", "^production"]
    },
    "e2e": {
      "cache": true,
      "dependsOn": ["build"],
      "inputs": [
        "default",
        "{projectRoot}/e2e/**/*",
        "{projectRoot}/**/*.e2e.{js,ts,jsx,tsx}"
      ]
    },
    "test:unit": {
      "cache": true,
      "outputs": ["{projectRoot}/coverage", "{projectRoot}/test-results"],
      "inputs": [
        "default",
        "{projectRoot}/test/**/*",
        "{projectRoot}/**/*.test.{js,ts,jsx,tsx}",
        "{projectRoot}/**/*.spec.{js,ts,jsx,tsx}",
        "{workspaceRoot}/vite.config.ts",
        "{workspaceRoot}/vitest.config.*"
      ]
    },
    "test:integration": {
      "cache": true,
      "outputs": ["{projectRoot}/coverage", "{projectRoot}/test-results"],
      "inputs": [
        "default",
        "{projectRoot}/test/**/*",
        "{projectRoot}/**/*.test.{js,ts,jsx,tsx}",
        "{projectRoot}/**/*.spec.{js,ts,jsx,tsx}",
        "{workspaceRoot}/vite.config.ts",
        "{workspaceRoot}/vitest.config.*"
      ]
    }
  },
  "plugins": [
    {
      "plugin": "@nx/eslint/plugin",
      "options": {
        "targetName": "lint"
      }
    },
    {
      "plugin": "@nx/vite/plugin",
      "options": {
        "buildTargetName": "build",
        "testTargetName": "test",
        "serveTargetName": "serve",
        "devTargetName": "dev",
        "previewTargetName": "preview",
        "serveStaticTargetName": "serve-static",
        "typecheckTargetName": "typecheck",
        "buildDepsTargetName": "build-deps",
        "watchDepsTargetName": "watch-deps"
      }
    }
  ],
  "nxCloudId": "6857fccbb755d4191ce6fbe4",
  "defaultBase": "main",
  "affected": {
    "defaultBase": "main"
  },
  "cli": {
    "analytics": false
  },
  "parallel": 6,
  "cacheDirectory": ".nx/cache"
}
