{
  "name": "eslint-config",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/eslint-config/src",
  "projectType": "library",
  "tags": ["scope:shared", "type:config"],
  "targets": {
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "packages/eslint-config/**/*.{js,mjs,ts}"
        ]
      }
    }
  }
}