{
  "name": "frbr",
  "root": "standards/FRBR",
  "sourceRoot": "standards/FRBR",
  "projectType": "application",
  "tags": ["docusaurus", "site", "standard", "frbr"],
  "implicitDependencies": ["@ifla/theme"],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "docusaurus build standards/FRBR"
      },
      "outputs": ["{projectRoot}/build"],
      "cache": true,
      "dependsOn": [
        "^build",
        {
          "target": "build",
          "projects": ["@ifla/theme"]
        }
      ],
      "inputs": ["production", "^production", "docusaurus-no-theme"]
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "command": "docusaurus start standards/FRBR --port 3005"
      },
      "cache": false
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "docusaurus serve standards/FRBR --port 3005"
      },
      "cache": false
    },
    "clear": {
      "executor": "nx:run-commands",
      "options": {
        "command": "docusaurus clear standards/FRBR"
      },
      "cache": false
    },
    "test": {
      "executor": "@nx/vite:test",
      "options": {
        "config": "{workspaceRoot}/vite.config.ts",
        "testPathPattern": ["{projectRoot}/src/**/*.{test,spec}.{js,ts,jsx,tsx}"]
      },
      "outputs": ["{projectRoot}/coverage"],
      "cache": true
    },
    "e2e": {
      "executor": "nx:run-commands",
      "options": {
        "command": "playwright test e2e/standards-smoke.spec.ts e2e/vocabulary-functionality.spec.ts"
      },
      "dependsOn": ["build"],
      "cache": true,
      "inputs": [
        "default",
        "{workspaceRoot}/e2e/standards-smoke.spec.ts",
        "{workspaceRoot}/e2e/vocabulary-functionality.spec.ts",
        "{workspaceRoot}/playwright.config.ts"
      ],
      "outputs": [
        "{workspaceRoot}/test-results",
        "{workspaceRoot}/playwright-report"
      ]
    }
  }
}
