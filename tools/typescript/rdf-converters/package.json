{
  "name": "@ifla/rdf-converters",
  "version": "0.1.0",
  "description": "RDF to CSV converters with DCTAP support for IFLA Standards Platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "rdf-to-csv": "tsx src/rdf-to-csv.ts",
    "rdf-folder-to-csv": "tsx src/rdf-folder-to-csv.ts",
    "rdf-to-mdx": "tsx src/rdf-to-mdx-pipeline.ts"
  },
  "dependencies": {
    "n3": "^1.17.2",
    "commander": "^11.1.0",
    "csv-parse": "^5.5.3",
    "csv-stringify": "^6.4.5",
    "jsonld": "^8.3.1",
    "rdfxml-streaming-parser": "^3.1.0",
    "glob": "^10.3.10",
    "chalk": "^5.3.0",
    "ora": "^8.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/jsonld": "^1.5.13",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@vitest/coverage-v8": "^1.1.3",
    "@vitest/ui": "^1.1.3",
    "eslint": "^8.56.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "rdf",
    "csv",
    "dctap",
    "ifla",
    "standards",
    "conversion"
  ],
  "author": "IFLA Standards Development Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/iflastandards/platform.git",
    "directory": "tools/typescript/rdf-converters"
  }
}